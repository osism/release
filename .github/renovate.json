{
  "extends": [
    "github>osism/renovate-config"
  ],
  "packageRules": [
    {
      "packageNames": ["moby"],
      "extractVersion": "^(?<version>v\\d+\\.\\d+)"
    }
  ],
  "regexManagers":[
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>ansible)_version: '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"pypi"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>ansible.netcommon): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>ansible.posix): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>arista.cvp): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>arista.eos): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>community.general): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>community.network): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>community.zabbix): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>debops.debops): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>netbox.netbox): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>openstack.cloud): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>juniper.device): '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"galaxy-collection"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "(?<depName>ara): '(?<currentValue>.*?)'",
        "(?<depName>ara)_server: '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate":"pypi"
    },
    {
      "fileMatch":[
        "^latest\\/base.yml$"
      ],
      "matchStrings":[
        "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\n.*?: '5:(?<currentValue>.*?)'"
     ]
    }
  ]
}
